#include <stdio.h>

int main()
{
    int num1, num2, resto;
    printf("Digite dois numeros inteiros:\n");
    scanf("%d%d", &num1, &num2);
    
    if (num1 > num2)
    {
        
        resto = num1 % num2;
    
        if (resto==0)
        {
            printf("%d eh multiplo de %d.", num1, num2);
        }
        else
        {
            printf("%d nao eh multiplo de %d.", num1, num2);
        }
    
    }
    else
    {
        resto = num2 % num1;
        if (resto==0)
        {
            printf("%d eh multiplo de %d.", num2, num1);
        }
        else
        {
            printf("%d nao eh multiplo de %d.", num2, num1);
        
        }
    }
    return 0;
}
