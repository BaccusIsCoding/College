#include <stdio.h>
#include <stdlib.h>

int main() {
    int num1, num2;
    printf("Digite dois numeros:\n");
    scanf("%d%d", &num1, &num2);
    (num1 >= num2)?
        (num1 == num2)?
            printf("Os numeros sao iguais")
            :
            printf("%d eh maior que %d", num1, num2)
        :
        printf("%d eh maior que %d", num2, num1);
}
